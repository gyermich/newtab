{
   "manifest_version": 2,
   "name": "my_new_tab",
   "description": "Apps and bookmarks on a new tab page",
   "version": "1.0",
   "permissions": ["management", "tabs"],
    "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [
    {
      "matches": ["http://jquery.com/*"],
      "js": ["jquery-2.1.0.js", "script.js", "jquery.gridster.js"]
    }
  ],
    "background": {
      "scripts": [ "script.js" ],
      "persistent": false
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   }
}